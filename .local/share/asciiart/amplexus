#!/bin/bash

f=3 b=4
for j in f b; do
	for i in {0..7}; do
		printf -v $j$i %b "\e[${!j}${i}m"
	done
done

cat <<EOF

$f1 █  ░█░░  █   $f2 █  ░█░░  █   $f3 █  ░█░░  █   $f4 █  ░█░░  █   $f5 █  ░█░░  █   $f6 █  ░█░░  █      
$f1██ O█░░█O ██  $f2██ O█░░█O ██  $f3██ O█░░█O ██  $f4██ O█░░█O ██  $f5██ O█░░█O ██  $f6██ O█░░█O ██     
$f1 █ ██░░██ █   $f2 █ ██░░██ █   $f3 █ ██░░██ █   $f4 █ ██░░██ █   $f5 █ ██░░██ █   $f6 █ ██░░██ █      
$f1 ██░░░░░░██   $f2 ██░░░░░░██   $f3 ██░░░░░░██   $f4 ██░░░░░░██   $f5 ██░░░░░░██   $f6 ██░░░░░░██      
$f1   ░█░░░░     $f2   ░█░░░░     $f3   ░█░░░░     $f4   ░█░░░░     $f5   ░█░░░░     $f6   ░█░░░░        
$f1 ██░█░░░░██   $f2 ██░█░░░░██   $f3 ██░█░░░░██   $f4 ██░█░░░░██   $f5 ██░█░░░░██   $f6 ██░█░░░░██      
$f1 █ █░█░░█ █   $f2 █ █░█░░█ █   $f3 █ █░█░░█ █   $f4 █ █░█░░█ █   $f5 █ █░█░░█ █   $f6 █ █░█░░█ █      
$f1██  █░░█  ██  $f2██  █░░█  ██  $f3██  █░░█  ██  $f4██  █░░█  ██  $f5██  █░░█  ██  $f6██  █░░█  ██     
$f1 █        █   $f2 █        █   $f3 █        █   $f4 █        █   $f5 █        █   $f6 █        █      

EOF
